{
    "name": "hansdeboeck/laravel-grapesjs",
    "description": "A package to easily integrate GrapesJS into your laravel proejct.",
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^12.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Hans De Boeck",
            "email": "hans@deboeck.dev"
        }
    ],
    "autoload": {
        "psr-4": {
            "HansDeBoeck\\Grapesjs\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HansDeBoeck\\Grapesjs\\GrapesjsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
